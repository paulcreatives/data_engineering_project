{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "CSV",
						"type": "DatasetReference"
					},
					"name": "SourceStream"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "Parquet",
						"type": "DatasetReference"
					},
					"name": "sink1",
					"rejectedDataLinkedService": {
						"referenceName": "AzureDataLake",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [
				{
					"name": "DataTypeCorrected",
					"description": "Autogenerated by data preview actions"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> SourceStream",
				"SourceStream derive(each(match(name=='ProductKey'), $$ = toInteger($$)),",
				"          each(match(name=='CustomerKey'), $$ = toInteger($$)),",
				"          each(match(name=='SalesTerritoryKey'), $$ = toInteger($$)),",
				"          each(match(name=='OrderQuantity'), $$ = toInteger($$)),",
				"          each(match(name=='UnitPrice'), $$ = toDouble($$)),",
				"          each(match(name=='TotalProductCost'), $$ = toDouble($$)),",
				"          each(match(name=='SalesAmount'), $$ = toDouble($$)),",
				"          each(match(name=='TaxAmt'), $$ = toDouble($$))) ~> DataTypeCorrected",
				"DataTypeCorrected sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 1) ~> sink1"
			]
		}
	}
}